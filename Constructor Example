public class ConstructorExample {
    int id;
    String name;

    ConstructorExample(int id, String name) {
        this.id = id;
        this.name = name;
    }

    void display() {
        System.out.println(id + " " + name);
    }

    public static void main(String[] args) {
        ConstructorExample obj = new ConstructorExample(101, "Anusha");
        obj.display();
    }
}
